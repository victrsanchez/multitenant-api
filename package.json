  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "nest build && node --max-old-space-size=6000 dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:generate": "ts-node node_modules/typeorm/cli.js migration:generate --config ./orm.config.json --name",
    "migration:create": "ts-node node_modules/typeorm/cli.js migration:create --config ./orm.config.json --name",
    "migration:run": "ts-node node_modules/typeorm/cli.js migration:run --config ./orm.config.json",
    "migration:revert": "ts-node node_modules/typeorm/cli.js migration:revert --config ./orm.config.json"
  },
